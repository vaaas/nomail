{
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2022",
    "baseUrl": ".",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "paths": {
      "/*": ["./src/*"]
    }
  },
  "include": ["src/**/*"]
}
